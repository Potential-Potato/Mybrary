<%- include('../partials/header.ejs') %>
<h2>Search Authors</h2>
    <form action="/authors" method="GET">
    <label>Name</label>
    <input type="text" name="searchName" value="<%= searchOptions.searchName %>">
    <button type="submit">Search</button>
    </form>
<br>

<% rAuthors.forEach(author => { %>
<div><%= author.name %></div>
<% }) %>